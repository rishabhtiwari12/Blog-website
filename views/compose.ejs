<%- include("partials/header"); -%>

<h1>Compose</h1>

<form action="/compose" method="post" autocomplete="off">
  <div class="mb-3">
    <label>Title</label>
    <input type="text" name="comp" class="form-control" />
    <label>Post</label>
    <textarea
      class="form-control"
      name="postBody"
      cols="30"
      rows="5"
    ></textarea>
  </div>
  <button style="margin-top: 15px;" class="btn btn-primary" type="submit" >Publish</button>
</form>

<%- include("partials/footer");-%>
